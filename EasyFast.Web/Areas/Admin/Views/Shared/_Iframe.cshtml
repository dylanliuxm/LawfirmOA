@using System.Threading
@using System.Web.Optimization
@using Abp.Timing
@using Abp.Web.Mvc.Extensions
@inherits EasyFast.Web.Views.EasyFastWebViewPageBase
@{
    SetAntiForgeryCookie();
}
<html>
<head>
    <meta charset="utf-8" />
    <title>京师在线系统后台管理</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta name="renderer" content="webkit" />
    <link href="http://cdn.easyfast.cn/assets/metronic4.7.1/global/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="http://cdn.easyfast.cn/assets/metronic4.7.1/global/plugins/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
    <link href="http://cdn.easyfast.cn/assets/bootstrap/bootstrap-3.3.7/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="http://cdn.easyfast.cn/assets/bootstrap/bootstrap-3.3.7/css/bootstrap-theme.min.css" rel="stylesheet" type="text/css" />
    <link href="http://cdn.easyfast.cn/assets/metronic4.7.1/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/site.min.css" rel="stylesheet" />

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="@Url.Content("~/Scripts/others/html5shiv.js")"></script>
        <script src="@Url.Content("~/Scripts/others/respond.min.js")"></script>
    <![endif]-->
    @Scripts.Render("~/Bundles/vendor/js/top")
    @RenderSection("styles", required: false)
</head>
<body class="iframe_box">

    @RenderBody()

    <script type="text/javascript">
        moment.locale('@Thread.CurrentThread.CurrentUICulture.Name');
        //This is used to get the application's root path from javascript. It's useful if you're running application in a virtual directory under IIS.
        var abp = abp || {}; abp.appPath = '@ApplicationPath';
    </script>

    <!--[if lt IE 9]>
    <script src="http://cdn.easyfast.cn/assets/metronic4.7.1/global/plugins/respond.min.js"></script>
    <script src="http://cdn.easyfast.cn/assets/metronic4.7.1/global/plugins/excanvas.min.js"></script>
    <script src="http://cdn.easyfast.cn/assets/metronic4.7.1/global/plugins/ie8.fix.min.js"></script>
    <![endif]-->
    <!-- BEGIN CORE PLUGINS -->
    <script src="http://cdn.easyfast.cn/assets/metronic4.7.1/global/plugins/jquery.min.js" type="text/javascript"></script>
    <script src="http://cdn.easyfast.cn/assets/metronic4.7.1/global/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="http://cdn.easyfast.cn/assets/metronic4.7.1/global/plugins/js.cookie.min.js" type="text/javascript"></script>
    <script src="http://cdn.easyfast.cn/assets/metronic4.7.1/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="http://cdn.easyfast.cn/assets/metronic4.7.1/global/plugins/jquery.blockui.min.js" type="text/javascript"></script>
    <script src="http://cdn.easyfast.cn/assets/metronic4.7.1/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js" type="text/javascript"></script>
    <!-- END CORE PLUGINS -->
    <!-- BEGIN THEME GLOBAL SCRIPTS -->
    <script src="http://cdn.easyfast.cn/assets/metronic4.7.1/global/scripts/app.min.js" type="text/javascript"></script>
    <!-- END THEME GLOBAL SCRIPTS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src="http://cdn.easyfast.cn/assets/metronic4.7.1/pages/scripts/dashboard.min.js" type="text/javascript"></script>
    <!-- END PAGE LEVEL SCRIPTS -->
    <!-- BEGIN THEME LAYOUT SCRIPTS -->
    <script src="http://cdn.easyfast.cn/assets/metronic4.7.1/layouts/layout2/scripts/layout.min.js" type="text/javascript"></script>
    <script src="http://cdn.easyfast.cn/assets/metronic4.7.1/layouts/layout2/scripts/demo.min.js" type="text/javascript"></script>
    <script src="http://cdn.easyfast.cn/assets/metronic4.7.1/layouts/global/scripts/quick-sidebar.min.js" type="text/javascript"></script>
    <script src="http://cdn.easyfast.cn/assets/metronic4.7.1/layouts/global/scripts/quick-nav.min.js" type="text/javascript"></script>
    <!-- END THEME LAYOUT SCRIPTS -->
    <!--插件-->
    <script src="http://cdn.easyfast.cn/assets/jquery-easyui-1.5/jquery.easyui.min.js" type="text/javascript"></script>
    <script src="http://cdn.easyfast.cn/assets/jquery-easyui-1.5/locale/easyui-lang-zh_CN.js" type="text/javascript"></script>
    <script src="http://cdn.easyfast.cn/assets/jquery-easyui-1.5/jquery.easyui.mobile.js" type="text/javascript"></script>
    <script src="http://cdn.easyfast.cn/assets/layer-v2.4/layer/layer.js" type="text/javascript"></script>
    <script src="~/api/AbpServiceProxies/GetAll?v=@(Clock.Now.Ticks)"></script>
    <script src="~/AbpScripts/GetScripts?v=@(Clock.Now.Ticks)" type="text/javascript"></script>
    <script src="~/Scripts/site.js"></script>
    @Html.IncludeScript("~/Abp/Framework/scripts/libs/abp.signalr.js")
    @RenderSection("scripts", required: false)
</body>
</html>
